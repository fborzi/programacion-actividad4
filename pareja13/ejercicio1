#Lista de la actividad
b = [4, 'palabra',[0,1],9.6,False]

#elementos a comprobar
elementos = [9.6,0,False,[0,1],4.0,'p']

#Comprobar si los elementos pertenecen a la lista 
for e in elementos:
    print(e, '->', e in b)

#Determinar el indice en la lista "b"
if [0,1] in b:
    print(f'El indice de [0,1] es:{b.index([0,1])}')
else:
    print('El elemento no se encuentra en la lista')

